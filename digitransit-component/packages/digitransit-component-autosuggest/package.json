{
  "name": "@digitransit-component/digitransit-component-autosuggest",
  "version": "1.5.8",
  "description": "digitransit-component autosuggest module",
  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:index": "ENV=production NAME=digitransit-component-autosuggest webpack --config ../webpack.config.babel.js && yarn run docs",
    "build:test": "babel test.js --presets=@babel/preset-react,@babel/preset-env --plugins=@babel/plugin-proposal-class-properties,@babel/plugin-proposal-numeric-separator --out-file test.generated",
    "watch": "ENV=development NAME=digitransit-component-autosuggest webpack --watch --config ../webpack.config.babel.js",
    "clean": "rm -rf ./lib ./node_modules",
    "test": "mocha -r esm test.generated",
    "docs": "node -r esm ../../scripts/generate-readmes"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/HSLdevcom/digitransit-ui.git"
  },
  "keywords": [
    "digitransit-component",
    "Autosuggest"
  ],
  "author": "Digitransit Authors",
  "license": "(AGPL-3.0 OR EUPL-1.2)",
  "dependencies": {
    "@digitransit-search-util/digitransit-search-util-execute-search-immidiate": "^0.2.13",
    "@digitransit-search-util/digitransit-search-util-get-label": "^0.1.0",
    "@digitransit-search-util/digitransit-search-util-uniq-by-label": "^0.1.0",
    "@hsl-fi/hooks": "^0.2.0"
  },
  "peerDependencies": {
    "@digitransit-component/digitransit-component-icon": "^0.0.15",
    "@digitransit-component/digitransit-component-dialog-modal": "^0.2.1",
    "@digitransit-component/digitransit-component-suggestion-item": "^0.2.1",
    "@hsl-fi/sass": "^0.1.1",
    "classnames": "2.2.6",
    "i18next": "^19.3.3",
    "lodash": "4.17.14",
    "lodash-es": "4.17.14",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-autosuggest": "^10.0.0",
    "react-modal": "~3.11.2",
    "recompose": "0.30.0"
  }
}
